{
  "name": "@rsuite/icons",
  "version": "1.0.2",
  "description": "All icons of rsuite",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.{tsx,ts,js}\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{tsx,ts,js}\"",
    "build": "npm run build:gulp && npm run build:types",
    "build:gulp": "gulp build",
    "build:sync-icon-component": "gulp buildIconComponent",
    "build:build-docs-list": "gulp buildIconListDocs",
    "build:types": "npx tsc --emitDeclarationOnly --outDir lib && npx tsc --emitDeclarationOnly --outDir es",
    "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3100 --host 0.0.0.0 ",
    "docs": "rm -rf assets &&  NODE_ENV=production webpack",
    "publish-docs": "node docs/gh-pages.js",
    "tdd": "karma start",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint": "npm run lint:ts",
    "test": "npm run lint && npm run run:test-case",
    "run:test-case": "cross-env NODE_ENV=test karma start --single-run",
    "clean": "rm -rf node_modules/gh-pages/.cache",
    "coveralls": "cat ./coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "cross-env PUBLISH=true npm run build",
    "prepare": "npm run build && npm run build:build-docs-list"
  },
  "author": "hiyangguo@gmail.com",
  "license": "MIT",
  "files": [
    "lib",
    "es",
    "dist",
    "legacy",
    "*.js",
    "*.d.ts",
    "meta.json"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:rsuite/rsuite-icons.git"
  },
  "dependencies": {
    "@rsuite/icon-font": "^4.0.0",
    "classnames": "^2.2.5",
    "insert-css": "^2.0.0",
    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-proto-to-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.1",
    "@types/classnames": "^2.2.10",
    "@types/insert-css": "^2.0.1",
    "@types/lodash": "^4.14.162",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^10.0.3",
